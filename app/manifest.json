{
    "name": "Popup to Content",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": [
            "js/content.js", 
            "content_script.js"]
    }],
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    }
}