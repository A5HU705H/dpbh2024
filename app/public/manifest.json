
{
  "manifest_version": 3,
  "name": "Deception Detectors",
  "version": "0.1.0",
  "description": "An app to detect dark patterns with various machine learning models with a wide variety of features.",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type":"module"
  },
  "action": {
    "default_title": "App",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "tabCapture",
    "<all_urls>",
    "idle"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js",
        "mutation.js",
        "inject.js"
      ]
    }
  ] ,
  "web_accessible_resources": [
    {
      "resources": [
        "pesticide.min.css",
        "pesticide.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
  

